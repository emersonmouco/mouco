programa {
  funcao inicio() {
    real frente, fundo, area
		faca{
			
			escreva("\nInforme a metragem da frente do terreno: ")
			leia(frente)
			escreva("\nInforme a metragem do comprimento do terreno: ")
			leia(fundo)
			area = frente * fundo
			escreva("\nÁrea do terreno : ",area)
			}enquanto(area>100)
  }
}
